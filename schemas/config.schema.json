{
    "$defs": {
        "V1BlackConfig": {
            "additionalProperties": false,
            "properties": {
                "formatter": {
                    "const": "black",
                    "title": "Formatter"
                },
                "options": {
                    "anyOf": [
                        {
                            "additionalProperties": {
                                "anyOf": [
                                    {
                                        "type": "string"
                                    },
                                    {
                                        "type": "integer"
                                    },
                                    {
                                        "type": "null"
                                    }
                                ]
                            },
                            "type": "object"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "title": "Options"
                },
                "config_path": {
                    "anyOf": [
                        {
                            "format": "path",
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "title": "Config Path"
                }
            },
            "required": [
                "formatter"
            ],
            "title": "V1BlackConfig",
            "type": "object"
        },
        "V1Config": {
            "additionalProperties": false,
            "properties": {
                "version": {
                    "const": "1",
                    "title": "Version"
                },
                "name": {
                    "title": "client name.",
                    "type": "string"
                },
                "output_dir": {
                    "anyOf": [
                        {
                            "format": "directory-path",
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "title": "output directory."
                },
                "source": {
                    "anyOf": [
                        {
                            "$ref": "#/$defs/V1OpenAPISource"
                        },
                        {
                            "items": {
                                "$ref": "#/$defs/V1OpenAPISource"
                            },
                            "type": "array"
                        }
                    ],
                    "default": [],
                    "title": "source settings."
                },
                "schema": {
                    "allOf": [
                        {
                            "$ref": "#/$defs/V1Schema"
                        }
                    ],
                    "default": {
                        "schema_type": "pydantic"
                    },
                    "title": "schema settings."
                },
                "format": {
                    "anyOf": [
                        {
                            "type": "boolean"
                        },
                        {
                            "$ref": "#/$defs/V1IsortConfig"
                        },
                        {
                            "$ref": "#/$defs/V1BlackConfig"
                        },
                        {
                            "items": {
                                "anyOf": [
                                    {
                                        "$ref": "#/$defs/V1IsortConfig"
                                    },
                                    {
                                        "$ref": "#/$defs/V1BlackConfig"
                                    }
                                ]
                            },
                            "type": "array"
                        }
                    ],
                    "default": true,
                    "title": "format generated code. default formatters are 'isort' and 'black'."
                },
                "lint": {
                    "anyOf": [
                        {
                            "type": "boolean"
                        },
                        {
                            "$ref": "#/$defs/V1RuffConfig"
                        },
                        {
                            "items": {
                                "$ref": "#/$defs/V1RuffConfig"
                            },
                            "type": "array"
                        }
                    ],
                    "default": true,
                    "title": "lint generated code. default linter is 'ruffo'"
                }
            },
            "required": [
                "version",
                "name"
            ],
            "title": "V1Config",
            "type": "object"
        },
        "V1IOpenAPI": {
            "additionalProperties": false,
            "properties": {
                "servers": {
                    "items": {
                        "$ref": "#/$defs/V1OpenAPIServer"
                    },
                    "title": "Servers",
                    "type": "array"
                }
            },
            "required": [
                "servers"
            ],
            "title": "V1IOpenAPI",
            "type": "object"
        },
        "V1IsortConfig": {
            "additionalProperties": false,
            "properties": {
                "formatter": {
                    "const": "isort",
                    "title": "Formatter"
                },
                "options": {
                    "anyOf": [
                        {
                            "additionalProperties": {
                                "anyOf": [
                                    {
                                        "type": "string"
                                    },
                                    {
                                        "type": "integer"
                                    },
                                    {
                                        "type": "null"
                                    }
                                ]
                            },
                            "type": "object"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "title": "Options"
                },
                "settings_path": {
                    "anyOf": [
                        {
                            "format": "path",
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "title": "Settings Path"
                }
            },
            "required": [
                "formatter"
            ],
            "title": "V1IsortConfig",
            "type": "object"
        },
        "V1OpenAPIServer": {
            "additionalProperties": false,
            "properties": {
                "url": {
                    "title": "Url",
                    "type": "string"
                }
            },
            "required": [
                "url"
            ],
            "title": "V1OpenAPIServer",
            "type": "object"
        },
        "V1OpenAPISource": {
            "additionalProperties": false,
            "properties": {
                "type": {
                    "const": "openapi",
                    "title": "Type"
                },
                "location": {
                    "anyOf": [
                        {
                            "format": "path",
                            "type": "string"
                        },
                        {
                            "format": "uri",
                            "maxLength": 2083,
                            "minLength": 1,
                            "type": "string"
                        }
                    ],
                    "title": "openapi file location."
                },
                "openapi": {
                    "anyOf": [
                        {
                            "$ref": "#/$defs/V1IOpenAPI"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "title": "server urls"
                }
            },
            "required": [
                "type",
                "location"
            ],
            "title": "V1OpenAPISource",
            "type": "object"
        },
        "V1RuffConfig": {
            "additionalProperties": false,
            "properties": {
                "linter": {
                    "const": "ruff",
                    "title": "Linter"
                },
                "options": {
                    "anyOf": [
                        {
                            "additionalProperties": {
                                "anyOf": [
                                    {
                                        "type": "string"
                                    },
                                    {
                                        "type": "integer"
                                    },
                                    {
                                        "type": "null"
                                    }
                                ]
                            },
                            "type": "object"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "title": "Options"
                },
                "config_path": {
                    "anyOf": [
                        {
                            "format": "path",
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "title": "Config Path"
                }
            },
            "required": [
                "linter"
            ],
            "title": "V1RuffConfig",
            "type": "object"
        },
        "V1Schema": {
            "additionalProperties": false,
            "properties": {
                "schema_type": {
                    "default": "pydantic",
                    "description": "`pydantic` or `typed_dict`.",
                    "enum": [
                        "pydantic",
                        "typed_dict"
                    ],
                    "title": "schema type.",
                    "type": "string"
                }
            },
            "title": "V1Schema",
            "type": "object"
        }
    },
    "allOf": [
        {
            "$ref": "#/$defs/V1Config"
        }
    ],
    "title": "Config"
}
